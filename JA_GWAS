 #Running vcfToBBF to convert all of the POP_filtered_chr#.recode.vcf files into their Plink formats for GWAS
 
    nano vcfToBBF
        plink --vcf POP_filtered_chr1.recode.vcf --out POP_filtered_chr1
        plink --vcf POP_filtered_chr2.recode.vcf --out POP_filtered_chr2
        plink --vcf POP_filtered_chr3.recode.vcf --out POP_filtered_chr3
        plink --vcf POP_filtered_chr4.recode.vcf --out POP_filtered_chr4
        plink --vcf POP_filtered_chr5.recode.vcf --out POP_filtered_chr5
        plink --vcf POP_filtered_chr6.recode.vcf --out POP_filtered_chr6
        plink --vcf POP_filtered_chr7.recode.vcf --out POP_filtered_chr7
        plink --vcf POP_filtered_chr8.recode.vcf --out POP_filtered_chr8
        plink --vcf POP_filtered_chr9.recode.vcf --out POP_filtered_chr9
        plink --vcf POP_filtered_chr10.recode.vcf --out POP_filtered_chr10
        plink --vcf POP_filtered_chr11.recode.vcf --out POP_filtered_chr11
        plink --vcf POP_filtered_chr12.recode.vcf --out POP_filtered_chr12
        plink --vcf POP_filtered_chr13.recode.vcf --out POP_filtered_chr13
        plink --vcf POP_filtered_chr14.recode.vcf --out POP_filtered_chr14
        plink --vcf POP_filtered_chr15.recode.vcf --out POP_filtered_chr15
        plink --vcf POP_filtered_chr16.recode.vcf --out POP_filtered_chr16
        plink --vcf POP_filtered_chr17.recode.vcf --out POP_filtered_chr17
        plink --vcf POP_filtered_chr18.recode.vcf --out POP_filtered_chr18
        plink --vcf POP_filtered_chr19.recode.vcf --out POP_filtered_chr19
        plink --vcf POP_filtered_chr20.recode.vcf --out POP_filtered_chr20
        
    bash vcfToBBF

